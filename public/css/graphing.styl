.graph-line-green
  fill: none
  stroke: green
  stroke-width: 1.5px
  opacity: 1

.graph-line-grey
  fill: none
  stroke: darkgrey
  stroke-width: 1.5px
  opacity: 1

.graph-line-opacity
  fill: none
  stroke: darkgrey
  stroke-width: 1.5px
  opacity: 0.2

.graph-line-highlighted
  fill: none
  stroke: black
  stroke-width: 1.5px
  opacity: 1

.axis path, .axis line
  fill: none
  stroke: #000
  shape-rendering: crispEdges

.line
  fill: none
  stroke: red
  stroke-width: 2px

.domain
  fill: none
  stroke: black
  width: 1px

.axisText
  font-family: "Verdana"
  font-size: 16px
  fill: black

.axis g text, #xAxisTickText, #yAxisTickText
  font-family: "Verdana"
  font-size: 14px
  fill: black

.peakCursors text
  font-family: "Verdana"
  font-size: 12px
  fill: black
  font-weight: bold
  text-anchor: middle

.peakCursors text.troughText
  font-style: italic

.boxPeakLabel
  font-family: "Verdana"
  font-size: 14px
  font-weight: bolder
  fill: black

.boxPeakNumber
  font-family: "Verdana"
  font-size: 14px
  fill: black
  font-weight: bold

.boxPeakValue
  font-family: "Verdana"
  font-size: 12px
  fill: black
  font-weight: normal