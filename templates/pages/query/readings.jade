.container
    h2 Query
        small &nbsp;ABR Readings
        | &nbsp;&nbsp;
        small: a(href='/query/sets').query-subtitle-link
            span.glyphicon.glyphicon-share-alt
            | &nbsp;ABR Sets
        | &nbsp;&nbsp;
        small: a(href='/query/groups').query-subtitle-link
            span.glyphicon.glyphicon-share-alt
            | &nbsp;ABR Groups
    .row
        .col-sm-6
            input.form-control#subjects
        .col-sm-6
            input.form-control#experiments
    hr
    table.table.table-hover
        thead
            th Date
            th Subject
            th Experiments
            th Tags
            th Freq (kHz)
            th Level (dB)
            th Latency?
            th Duration (ms)
            th Sample Rate
            th Samples
            th Max (uV)
            th Min (uV)
        tbody(data-hook='readings-table')
    hr
    .row(data-hook='pagination-control')

